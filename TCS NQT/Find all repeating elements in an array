1st Way: Using two for loops
Time Complexity: O(n^2)+O(n) Space Complexity: O(1)

#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin>>n;
	
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
  
  int cnt = 0;
    int dup[n];
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i]==arr[j]){
                dup[cnt++] = arr[i];
            }
        }
    }
    
    for(int i=0;i<cnt;i++){
        if(dup[i]!=dup[i+1]){
            cout<<dup[i];
        }
    }
}

2nd Way: Using Sorting
Time Complexity: O(nlogn)+O(n)

#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin>>n;
	
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
  
  sort(arr,arr+n);
    
  for(int i=0;i<n;i++){
      if(arr[i]==arr[i+1]){
          cout<<arr[i];
      }
  }
}

3rd Way: Uisng hashmap
Time Comlexity: O(n) Space Complexity: O(n)

#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin>>n;
	
	int arr[n];
	for(int i=0;i<n;i++){
	    cin>>arr[i];
	}
  
  unordered_map<int,int> mp;
    for(auto i:arr){
        mp[i]++;
    }
    
    for(auto i:mp){
        if(i.second>1){
            cout<<i.first;
        }
    }
}
